<div class="group-list">
    {% if header.label is defined and header.label is not empty %}
        <h3 class="title-radio-list" title="{{ header.title }}">{{ header.label }}</h3>
    {% endif %}
    {% for group in items %}
        <fieldset>
            {% if group is defined and group is not empty %}
                {% include 'atoms-legend' with group.legend|merge({'label': ('places.autocomplete.title.' ~ group.legend.type)|trans('messages', {'%value%': ''})}) only %}
            {% endif %}
            {% include 'atoms-list-standard' with {'items': group.items, 'attributes': group.attributes} only %}
        </fieldset>
    {% endfor %}
</div>
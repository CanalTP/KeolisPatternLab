<div class="col-4 col-md-6">
    <div class="class="st0">
        <form id="" action="#" name="search">
            <fieldset class="journey-specifics">
                <legend class="hide">Vos critères de recherche</legend>
                <div class="clearfix">
                    <!-- Btn inverser -->
                    <div class="journey-reverse">
                        <a class="link-reverse" href="#" title="Inverser le départ et l'arrivée">
                            <span class="sr-only">Inverser le départ et l'arrivée</span>
                            <span class="keolis-icon-reverse-2"></span>
                        </a>
                    </div>
                    <div class="clearfix group-field-container icon-left">
                        <!-- Départ -->
                        <div class="field-container clearfix">
                            <label class="hide" for="text">Text Input <abbr title="Required">*</abbr></label>
                            <div class="icon-inside-input left">
                                <span class="inner-iput-text" title="Partir Depuis ">De</span>
                                <input id="text" type="text" placeholder="Saisir un arrêt, une adresse, une communue" class="inner-icons">
                                <ul class="list-input-icons">
                                    <li><a title="Se géolocaliser" href="#" class="" id="geolocButton">
                                        <span class="keolis-icon-geoloc"></span>
                                        <span class="sr-only">Se géolocaliser</span>
                                    </a></li>
                                    <li><a title="Ajouter en favoris" href="#" class="input-icons pos-right" id="favoris">
                                        <span class="keolis-icon-star-empty"></span>
                                        <span class="sr-only">favoris</span>
                                    </a></li>
                               </ul>
                            </div>
                        </div>
                        <!-- arrivee -->
                        <div class="field-container clearfix">
                            <label class="hide" for="text">Text Input <abbr title="Required">*</abbr></label>
                            <div class="icon-inside-input left">
                                <span class="inner-iput-text" title="Arrivée vers ">Vers</span>
                                <input id="text" type="text" placeholder="Saisir un arrêt, une adresse, une communue" class="inner-icons">
                                <ul class="list-input-icons">
                                    <li><a title="Ajouter en favoris" href="#" class="input-icons pos-right" id="favoris">
                                        <span class="keolis-icon-star-empty"></span>
                                        <span class="sr-only">favoris</span>
                                    </a></li>
                               </ul>
                            </div>
                        </div>
                    </div>
                </div>
               {{> molecules-search-datetime }}
               {{> organisms-checkbox-accordion }}
               <div class="groupe-btn">
                    <button type="button" class="btn secondary full-width" title="valider le formulaire">
                         Rechercher
                    </button>
               </div>
            </fieldset>
        </form>
    </div>
</div>
